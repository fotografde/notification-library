{
    "name": "fotografde/notification-library",
    "version": "1.1.0",
    "homepage": "https://dev.getphoto.io/getphoto/NotificationLibrary",
    "description": "A simple library to push events events to the queue to be handled by the Notification Service",
    "keywords": ["events", "notification service"],
    "type": "library",
    "require": {
        "php": ">=5.5",
        "aws/aws-sdk-php": "~3.66.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "GetPhoto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GetPhoto\\Test\\": "tests/"
        }
    }
}
